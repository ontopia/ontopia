  <topic id="variant-in-basename">
    <instanceOf>
      <topicRef xlink:href="#issue"></topicRef>
    </instanceOf>
    <subjectIdentity>
      <subjectIndicatorRef xlink:href="http://psi.ontopia.net/iso13250/issue/#variant-in-basename"></subjectIndicatorRef>
