  <topic id="locator-normalization">
    <instanceOf>
      <topicRef xlink:href="#issue"></topicRef>
    </instanceOf>
    <subjectIdentity>
      <subjectIndicatorRef xlink:href="http://psi.ontopia.net/iso13250/issue/#locator-normalization"></subjectIndicatorRef>
    </subjectIdentity>
