    </occurrence>
    <occurrence>
      <instanceOf>
        <topicRef xlink:href="#background"></topicRef>
      </instanceOf>
      <resourceRef xlink:href="http://www.apache.org/~fielding/uri/rev-2002/issues.html#017-rdf-fragment"></resourceRef>
    </occurrence>
    <occurrence>
      <instanceOf>
        <topicRef xlink:href="#background"></topicRef>
      </instanceOf>
      <resourceRef xlink:href="http://lists.w3.org/Archives/Public/www-xml-linking-comments/2001AprJun/0158.html"></resourceRef>
