<topicMap id="xtm-language"
   xmlns="http://www.topicmaps.org/xtm/1.0/"
   xmlns:xlink='http://www.w3.org/1999/xlink'> 

  <topic id="containee">
    <baseName id="containee_name">
      <baseNameString>Disallowed Topic</baseNameString>
    </baseName>
  </topic>
  
  <topic id="container">
    <baseName id="container_name">
      <baseNameString>Container Name</baseNameString>
    </baseName>
  </topic>
  
  <topic id="norway">
    <baseName id="norway_name">
      <baseNameString>Norway</baseNameString>
    </baseName>
  </topic>

  <topic id="oslo">
    <baseName id="oslo_name">
      <baseNameString>Oslo</baseNameString>
    </baseName>
  </topic>

<association>
    <!--instanceOf>
      <topicRef xlink:href="#contained-in"/>
    </instanceOf-->
    <member>
      <roleSpec>
        <topicRef xlink:href="#containee"/>
      </roleSpec>
      <topicRef xlink:href="#oslo"/>
    </member>
    <member>
      <roleSpec>
        <topicRef xlink:href="#container"/>
      </roleSpec>
      <topicRef xlink:href="#norway"/>
    </member>
  </association>


  <!-- have to add this element so that default name type will get a
  deterministic ID after reimport -->
<topic id="defnametype">
  <subjectIdentity>
    <subjectIndicatorRef xlink:href="http://psi.topicmaps.org/iso13250/model/topic-name"/>
  </subjectIdentity>
</topic>
</topicMap>
